{
    "name": "catfishphp/noop",
    "description": "Robust and tested implementation of a no operation function",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "David Stanley",
            "email": "davidstanley01@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-deprecation-rules": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "phpstan/phpstan-strict-rules": "^0.10",
        "phpunit/phpunit": "^7.1",
        "doctrine/coding-standard": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Catfish\\Noop\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/noop.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "sniff": "vendor/bin/phpcs --standard=.phpcs.xml -n --extensions=php src tests"
    }
}
